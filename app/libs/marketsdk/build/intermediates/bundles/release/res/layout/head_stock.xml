<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="#2E2E2E"
    android:orientation="vertical">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center">

        <TextView
            android:id="@+id/open"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="10dp"
            android:layout_marginLeft="10dp"
            android:textSize="20sp" />

        <TextView
            android:id="@+id/bi_zhi_01"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/open"
            android:layout_marginLeft="10dp"
            android:textSize="12sp" />

        <TextView
            android:id="@+id/bizhi_02"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignTop="@id/bi_zhi_01"
            android:layout_below="@id/open"
            android:layout_marginLeft="5dp"
            android:layout_toRightOf="@id/bi_zhi_01"
            android:textSize="12sp" />

        <TextView
            android:id="@+id/jin_kai"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignTop="@id/open"
            android:layout_marginLeft="30dp"
            android:layout_toRightOf="@id/open"
            android:textColor="#808080"
            android:textSize="12sp" />

        <TextView
            android:id="@+id/jin_kai_jia"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignTop="@id/jin_kai"
            android:layout_marginLeft="5dp"
            android:layout_toRightOf="@id/jin_kai"
            android:textSize="12sp" />

        <TextView
            android:id="@+id/gao"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignTop="@id/jin_kai_jia"
            android:layout_marginLeft="10dp"
            android:layout_toRightOf="@id/jin_kai_jia"
            android:textColor="#808080"
            android:textSize="12sp" />

        <TextView
            android:id="@+id/gao_zhi"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignTop="@id/gao"
            android:layout_marginLeft="5dp"
            android:textColor="@color/white"
            android:layout_toRightOf="@id/gao"
            android:textSize="12sp" />

        <TextView
            android:id="@+id/zuo_shou"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@id/jin_kai"
            android:layout_alignTop="@id/bi_zhi_01"
            android:layout_below="@id/jin_kai"
            android:textColor="#808080"
            android:textSize="12sp" />

        <TextView
            android:id="@+id/zuo_shou_zhi"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@id/jin_kai_jia"
            android:layout_alignTop="@id/zuo_shou"
            android:textColor="@color/white"
            android:layout_toRightOf="@id/zuo_shou"
            android:textSize="12sp" />

        <TextView
            android:id="@+id/di"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@id/gao"
            android:layout_alignTop="@id/zuo_shou"
            android:layout_below="@id/gao"
            android:textColor="#808080"
            android:textSize="12sp" />

        <TextView
            android:id="@+id/di_zhi"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@id/gao_zhi"
            android:layout_alignTop="@id/di"
            android:layout_toRightOf="@id/di"
            android:textColor="@color/white"
            android:textSize="12sp" />

        <ImageView
            android:id="@+id/image_jia"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="20dp"
            android:layout_toRightOf="@id/di_zhi"/>
    </RelativeLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="10dp"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/xian_shou"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:textColor="#808080"
            android:textSize="12sp" />

        <TextView
            android:id="@+id/xian_shou_zhi"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:ellipsize="end"
            android:textColor="@color/white"
            android:singleLine="true"
            android:textSize="12sp" />

        <TextView
            android:id="@+id/cheng_jiao_ling"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:textColor="#808080"
            android:textSize="12sp" />

        <TextView
            android:id="@+id/cjl_zhi"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:textColor="@color/white"
            android:ellipsize="end"
            android:singleLine="true"
            android:textSize="10sp" />

        <TextView
            android:id="@+id/s_y_l"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:textColor="#808080"
            android:textSize="12sp" />

        <TextView
            android:id="@+id/s_y_l_zhi"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:ellipsize="end"
            android:textColor="@color/white"
            android:singleLine="true"
            android:textSize="12sp" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="10dp"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/liang_bi"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"

            android:textColor="#808080"
            android:textSize="12sp" />

        <TextView
            android:id="@+id/liang_bi_zhi"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:ellipsize="end"
            android:textColor="@color/white"
            android:singleLine="true"
            android:textSize="12sp" />

        <TextView
            android:id="@+id/cheng_jiao_e"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:textColor="#808080"
            android:textSize="12sp" />

        <TextView
            android:id="@+id/cheng_jiao_e_zhi"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:ellipsize="end"
            android:singleLine="true"
            android:textColor="@color/white"
            android:textSize="12sp" />

        <TextView
            android:id="@+id/shou_yi"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:textColor="#808080"
            android:textSize="12sp" />

        <TextView
            android:id="@+id/shou_yi_zhi"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:ellipsize="end"
            android:singleLine="true"
            android:textColor="@color/white"
            android:textSize="12sp" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="10dp">

        <TextView
            android:id="@+id/huang_shou"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:textColor="#808080"
            android:textSize="12sp" />

        <TextView
            android:id="@+id/huan_shou_zhi"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:ellipsize="end"
            android:singleLine="true"
            android:textColor="@color/white"
            android:textSize="12sp" />

        <TextView
            android:id="@+id/zong_shi"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:textColor="#808080"
            android:textSize="12sp" />

        <TextView
            android:id="@+id/zong_shi_zhi"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:ellipsize="end"
            android:textColor="@color/white"
            android:singleLine="true"
            android:textSize="12sp" />

        <TextView
            android:id="@+id/liu_tong"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:textColor="#808080"
            android:textSize="12sp" />

        <TextView
            android:id="@+id/liu_tong_zhi"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:ellipsize="end"
            android:textColor="@color/white"
            android:singleLine="true"
            android:textSize="12sp" />
    </LinearLayout>


</LinearLayout>